        {
            show_debug_message("Clicked on play game");
            room_goto(setupRoom);
        }
    
    
        if (mouse_x &gt;= 500 &amp;&amp; mouse_x &lt;= 640 &amp;&amp; mouse_y &gt;= 600 &amp;&amp; mouse_y &lt;= 640)
        {
            show_debug_message("Clicked on editor");
            room_goto(editorMenuScreen);
        }  
    }
    else
    {
        if (mouse_x &gt;= 400 &amp;&amp; mouse_x &lt;= 540 &amp;&amp; mouse_y &gt;= 600 &amp;&amp; mouse_y &lt;= 640)
        {
            show_debug_message("Clicked on play game");
            room_goto(setupRoom);
        }
    }
}

//quit game
